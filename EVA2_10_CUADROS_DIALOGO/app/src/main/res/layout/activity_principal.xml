<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context="com.example.iowner.eva2_10_cuadros_dialogo.Principal">

    <Button
        android:layout_width="match_parent"
        android:onClick="general"
        android:layout_height="wrap_content"
        android:text="CUADRO DE DIALOGO GENERAL" />

    <Button
        android:id="@+id/button"
        android:onClick="personalizado"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CUADRO DE DIALOGO PERSONALIZADO" />
</LinearLayout>
